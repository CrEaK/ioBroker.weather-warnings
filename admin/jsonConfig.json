            "items": {
                "templateTable": {
                    "type": "table",
                    "newLine": true,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "label": "",
                    "showSecondAddAt": 5,
                    "items": [
                        {
                            "type": "text",
                            "attr": "templateKey",
                            "width": "10% ",
                            "title": "Unique Identifier",
                            "tooltip": "Enter a unique text",
                            "filter": false,
                            "sort": false,
                            "default": "",
                            "validator": "const x={};for (const c of globalData.templateTable) {if (x[c.templateKey]==='x') return false; x[c.templateKey]='x';}; return true;",
                            "validatorNoSaveOnError": true
                        },
                        {
                            "type": "text",
                            "attr": "template",
                            "width": "90% ",
                            "title": "Text",
                            "tooltip": "Enter your template text.",
                            "filter": false,
                            "sort": false,
                            "default": ""
                        }
                    ]
                }
            }
