                "zamgEnabled": {
                    "type": "checkbox",
                    "sm": 6,
                    "md": 4,
                    "lg": 2,
                    "label": "Activate ZAMG",
                    "newLine": true
                },
                "refreshTime": {
                    "type": "number",
                    "label": "Update interval for all providers. (in minutes)",
                    "min": 5,
                    "step": 1,
                    "sm": 8,
                    "md": 4,
                    "lg": 2
                },
                "dividerHdr": {
                    "newLine": true,
                    "type": "header",
                    "text": "Testing",
